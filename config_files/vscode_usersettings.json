{
    "[markdown]": {
        "editor.autoClosingBrackets": "never",
        "editor.quickSuggestions": {
            "comments": "off",
            "other": "off",
            "strings": "off"
        },
        "editor.wordWrap": "on"
    },
    "[plaintext]": {
        "editor.autoClosingBrackets": "never",
        "editor.quickSuggestions": {
            "comments": "off",
            "other": "off",
            "strings": "off"
        }
    },
    "breadcrumbs.enabled": true,
    "cSpell.customDictionaries": {},
    "cSpell.ignoreWords": [],
    "cSpell.userWords": [
        "venv"
    ],
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "jupyter.askForKernelRestart": false,
    "jupyter.runStartupCommands": [
        "%load_ext autoreload",
        "%autoreload 2"
    ],
    "notebook.diff.ignoreMetadata": true,
    "notebook.diff.ignoreOutputs": true,
    "outline.showConstructors": false,
    "outline.showVariables": false,
    "powershell.enableReferencesCodeLens": false,
    "powershell.integratedConsole.showOnStartup": false,
    "powershell.integratedConsole.startInBackground": true,
    "powershell.integratedConsole.suppressStartupBanner": true,
    "powershell.powerShellDefaultVersion": "Windows PowerShell (x64)",
    "python.analysis.extraPaths": [],
    "python.analysis.inlayHints.functionReturnTypes": true,
    "python.analysis.inlayHints.variableTypes": false,
    "python.analysis.typeCheckingMode": "standard",
    "statsBar.modules": [
        "cpuLoad",
        "networkSpeed",
        "memoUsage",
        "uptime"
    ],
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "terminal.integrated.fontFamily": "MesloLGM Nerd Font",
    "terminal.integrated.hideOnStartup": "never",
    "terminal.integrated.inheritEnv": true,
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "args": [
                "-nologo"
            ],
            "source": "PowerShell"
        }
    },
    "vscode-pets.petSize": "medium",
    "vscode-pets.throwBallWithMouse": true,
    "window.confirmSaveUntitledWorkspace": false,
    "workbench.colorCustomizations": {
        "tab.activeBackground": "#6d3e3e",
        "tab.activeBorderTop": "#e70a2f"
    },
    "workbench.iconTheme": "vscode-icons",
    "workbench.startupEditor": "none"
}